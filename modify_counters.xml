<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/tableLayout"
        tools:ignore="UselessParent">
        <TableRow>
            <TextView
                android:text="Machine ID:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_machine"
                android:textSize="28sp"
                android:textStyle="bold|italic"
                android:textAlignment="center"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_Id"
                android:ems="4"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp"/>
        </TableRow>
        <TableRow>
            <TextView
                android:text="Total In:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_totalIn"
                android:textSize="28sp"
                android:textStyle="bold|italic"
                android:textAlignment="center"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_TotalIn"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp"/>
        </TableRow>
        <TableRow>
            <TextView
                android:text="Total Out:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_totalOut"
                android:cursorVisible="true"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                android:textSize="28sp"
                tools:ignore="HardcodedText,TextViewEdits" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_TotalOut"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp" />
            <TextView
                android:text="                      "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_space1"
                android:textSize="18sp"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                tools:ignore="HardcodedText" />
        </TableRow>
        <TableRow>
            <TextView
                android:text="Hand Paid:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_handPaid"
                android:textSize="28sp"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_HP"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp" />
        </TableRow>
        <TableRow>
            <TextView
                android:text="Bill:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_bill"
                android:textAlignment="center"
                android:textSize="28sp"
                android:textStyle="bold|italic"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_Bill"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp" />
        </TableRow>
        <TableRow>
            <TextView
                android:text="Jackpot:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_jackpot"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                android:textSize="28sp"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_TicketIn"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp"/>
        </TableRow>
        <TableRow>
            <TextView
                android:text="@string/ticket_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_ticketIn"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                android:textSize="28sp"
                tools:ignore="HardcodedText"/>
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_Jackpot"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp"/>
            <TextView
                android:text="                      "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_space2"
                android:textSize="18sp"
                android:textAlignment="center"
                android:textStyle="bold|italic"
                tools:ignore="HardcodedText" />

            <Button
                android:text="@string/save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold|italic"
                android:textSize="28sp"
                android:id="@+id/btn_save"
                android:textAlignment="center"
                android:width="300dp" />
        </TableRow>
        <TableRow>
            <TextView
                android:text="@string/ticket_out"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_ticketOut"
                android:textAlignment="center"
                android:textSize="28sp"
                android:textStyle="bold|italic"
                tools:ignore="HardcodedText" />
            <AutoCompleteTextView
                android:text="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/actv_TicketOut"
                android:ems="12"
                android:inputType="number"
                android:lines="1"
                android:textAlignment="center"
                android:textColor="?android:attr/colorForeground"
                android:cursorVisible="true"
                tools:ignore="HardcodedText"
                android:textSize="28sp" />
        </TableRow>
    </TableLayout>
</RelativeLayout>